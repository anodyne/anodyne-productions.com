        "@popperjs/core": "^2.1.0",
        "@tailwindcss/ui": "^0.1.3",
        "axios": "^0.19",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "postcss-import": "^12.0.1",
        "postcss-nested": "^4.2.1",
        "resolve-url-loader": "^3.1.0",
        "tailwindcss": "^1.2.0",
        "vue": "^2.6.11",
        "vue-template-compiler": "^2.6.11"
